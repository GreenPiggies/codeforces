#include <bits/stdc++.h>
using namespace std;

int main() {
	cin.tie(0)->sync_with_stdio(0);
	int t; cin >> t;
	while (t--) {
		int n; cin >> n;
		vector<int> a(n);
		for (int& x : a) cin >> x;

		sort(a.begin(), a.end());
		int ans = 0;
		for (int i = n / 2; i < n; i++) {
			if (a[i] == a[n / 2]) ans++;
		}
		cout << ans << '\n';
	}
	return 0;
}
